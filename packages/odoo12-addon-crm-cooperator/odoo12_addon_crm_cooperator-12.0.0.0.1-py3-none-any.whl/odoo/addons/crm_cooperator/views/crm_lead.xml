<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
  <data>
    <record id="view_crm_cooperator_crm_lead_form" model="ir.ui.view">
      <field name="name">crm.cooperator.crm.lead.form</field>
      <field name="model">crm.lead</field>
      <field name="type">form</field>
      <field name="inherit_id" ref="crm.crm_case_form_view_oppor"/>
      <field name="arch" type="xml">
        <xpath
          expr="//field[@name='team_id']"
          position="replace"
        >
          <field name="team_id"/>
          <field name="subscription_request_id"/>
        </xpath>
      </field>
     </record>
  </data>
</odoo>
