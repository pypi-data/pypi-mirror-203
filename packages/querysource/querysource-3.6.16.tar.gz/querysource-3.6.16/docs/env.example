CONFIG_FILE=etc/config.ini

[api]
API_HOST=qsapi.dev.local:5000

[database]
DBENGINE=navigator.pgschema
DBUSER=qs_pgdata
DBPWD=12345678
DBHOST=localhost
DBPORT=5432
DBNAME=qs_dev

[rethinkdb]
USE_RT=true
RT_HOST=localhost
RT_PORT=28015
RT_NAME=qs
RT_USER=admin
RT_PWD=

[mssql]
MSSQL_DRIVER=sqlserver
MSSQL_HOST=localhost
MSSQL_PORT=1433
MSSQL_USER=sa
MSSQL_PWD=12345678
MSSQL_DATABASE=AdventureWorks2019

[cache]
CACHEHOST=localhost
CACHEPORT=6379
QUERYSET_DB=0
MEMCACHE_HOST=localhost
MEMCACHE_PORT=11211
CACHE_PREFIX=local

[session]
SESSION_STORAGE=redis
SESSION_PREFIX=navapi

[debug]
PRODUCTION=false
DEBUG=true
TEMPLATE_DEBUG=true
SKIP_CSRF_MIDDLEWARE=true
INTERNAL_IPS=127.0.0.1
HOSTS=127.0.0.1, qsapi.dev.local, qsapi.dev.local:5000, localhost, dev.dev.local:8000

[newrelic]
NEW_RELIC_API_KEY=
NEW_RELIC_ACCOUNT=

[hubspot]
HUBSPOT_API_KEY=
HUBSPOT_PORTAL_ID=

[icims]
ICIMS_API_KEY=
ICIMS_API_SECRET=
ICIMS_API_USERNAME=
ICIMS_API_PASSWORD=

[Salesforce]
SALESFORCE_INSTANCE=
SALESFORCE_TOKEN=
SALESFORCE_DOMAIN=
SALESFORCE_USERNAME=
SALESFORCE_PASSWORD=
