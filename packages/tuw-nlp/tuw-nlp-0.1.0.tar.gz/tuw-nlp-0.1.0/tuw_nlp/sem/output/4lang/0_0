	dpi=140;
	0 [shape = circle, label = "dump"];
	1 [shape = circle, label = "spam"];
	2 [shape = circle, label = "we"];
	3 [shape = circle, label = "into"];
	4 [shape = circle, label = "folder"];
	5 [shape = circle, label = "junk"];
	0 -> 1 [ label = "2" ];
	0 -> 2 [ label = "1" ];
	3 -> 0 [ label = "1" ];
	3 -> 4 [ label = "2" ];
	4 -> 5 [ label = "0" ];
}
